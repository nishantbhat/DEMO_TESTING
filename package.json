{
  "name": "ucw",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "child_process": "^1.0.2",
    "cross-env": "5.0.1",
    "electron-is-dev": "0.1.2",
    "electron-log": "^2.2.6",
    "electron-updater": "^2.4.3",
    "node-notifier": "^5.1.2",
    "react": "15.5.4",
    "react-bootstrap": "^0.31.5",
    "react-bootstrap-table": "^4.1.1",
    "react-breadcrumbs": "^1.6.6",
    "react-breadcrumbs-dynamic": "^1.0.9",
    "react-dom": "15.5.4",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "react-router-breadcrumbs": "^2.1.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.53",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2017-node7": "^0.5.2",
    "babel-preset-react": "^6.23.0",
    "chalk": "^1.1.3",
    "concurrently": "3.4.0",
    "electron": "1.6.10",
    "electron-builder": "^19.37.2",
    "electron-devtools-installer": "^2.1.0",
    "electron-packager": "^9.1.0",
    "electron-prebuilt-compile": "^1.6.2",
    "enzyme": "^3.2.0",
    "eslint": "^3.18.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-watch": "^3.0.1",
    "jest": "^21.2.1",
    "jsdom": "11.4.0",
    "jsdom-global": "3.0.2",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.4.4",
    "react-datatable": "0.0.31",
    "react-scripts": "1.0.7",
    "wait-on": "2.0.2"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "com.crossmatch.ucw",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "publish": {
      "provider": "github"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build --em.main=build/electron.js",
    "electron-release": "build --em.main=build/electron.js --publish=always",
    "preelectron-pack": "yarn build",
    "preelectron-release": "yarn build"
  }
}
